{
  "MEMPOOL": {
    "NETWORK": "{{ network | default('mainnet') }}",
    "BACKEND": "electrum",
    "HTTP_PORT": 8999,
    "SPAWN_CLUSTER_PROCS": 0,
    "API_URL_PREFIX": "/api/v1/",
    "POLL_RATE_MS": 2000,
    "CACHE_DIR": "/var/lib/mempool/cache",
    "CLEAR_PROTECTION_MINUTES": 20,
    "RECOMMENDED_FEE_PERCENTILE": 50,
    "BLOCK_WEIGHT_UNITS": 4000000,
    "INITIAL_BLOCKS_AMOUNT": 8,
    "MEMPOOL_BLOCKS_AMOUNT": 8,
    "INDEXING_BLOCKS_AMOUNT": 11000,
    "PRICE_FEED_UPDATE_INTERVAL": 600,
    "USE_SECOND_NODE_FOR_MINFEE": false,
    "EXTERNAL_ASSETS": []
  },
  "CORE_RPC": {
    "HOST": "{{ CORE_RPC_HOST | default('127.0.0.1') }}",
    "PORT": {{ CORE_RPC_PORT | default(8332) }},
    "USERNAME": "{{ CORE_RPC_USERNAME }}",
    "PASSWORD": "{{ CORE_RPC_PASSWORD }}",
    "TIMEOUT": 60000
  },
  "ELECTRUM": {
    "HOST": "{{ ELECTRUM_HOST | default('127.0.0.1') }}",
    "PORT": {{ ELECTRUM_PORT | default(50001) }},
    "TLS_ENABLED": false
  },
  "DATABASE": {
    "ENABLED": true,
    "HOST": "127.0.0.1",
    "PORT": 3306,
    "DATABASE": "{{ MYSQL_DATABASE | default('mempool') }}",
    "USERNAME": "{{ MYSQL_USER }}",
    "PASSWORD": "{{ MYSQL_PASSWORD }}"
  },
  "STATISTICS": {
    "ENABLED": true,
    "TX_PER_SECOND_SAMPLE_PERIOD": 150
  },
  "SOCKS5PROXY": {
    "ENABLED": false
  },
  "PRICE_DATA_SERVER": {
    "TOR_URL": ""
  },
  "EXTERNAL_DATA_SERVER": {
    "MEMPOOL_API": "",
    "MEMPOOL_API_TOR": "",
    "LIQUID_API": "",
    "LIQUID_API_TOR": "",
    "BISQ_URL": "",
    "BISQ_URL_TOR": ""
  }
}
